{
    "commands": [{
        "what": "OneApiService",
        "handler": "step1",
        "apiinfo": {
            "oa": {
                "oauth2": "true",
                "oauthurl": "https://api.dropbox.com/1/oauth2/token",
                "requesttokenurl": "https://www.dropbox.com/1/oauth2/authorize",
                "key1": "xxxxxxxxxxxxx",
                "key2": "xxxxxxxxxxxxxxxx",
                "version": "1.0",
                "callbackurl": "http://localhost:3000/",
