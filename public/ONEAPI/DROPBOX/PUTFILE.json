                "finalurl": "https://api-content.dropbox.com/1/files_put/auto/hello.txt",
                "parameters": {
                    "redirect_uri": "http://localhost:3000/",
                    "response_type": "code"
                },
                "step2parameters": {
                    "redirect_uri": "http://localhost:3000/",
                    "grant_type": "authorization_code"
                },
                "finalurldata": {
                    "param": "val"
                },
                "finalrequestbody": "This is the text that will be written to the file",
                "finalrequestheaders": {
                    "Content-Type": "text/plain"
                },
                "finalurlmethod": "PUT"
            }
        }
    }]
